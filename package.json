    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "html-pdf": "^2.2.0",
    "jquery": "^3.3.1",
    "jsdom": "^11.6.2",
    "parsoid": "^0.9.0",
    "path": "^0.12.7",
    "request": "^2.83.0"
  },
  "devDependencies": {},
  "scripts": {
    "postinstall": "node ./install.js",
    "service": "node ./index.js"
  },
